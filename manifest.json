{
	"name": "Patostreamy.com Notifier",
	"version": "1.3.1",
	"manifest_version": 3,
	"description": "Śledź na żywo najpopularniejszych patostreamerów w Polsce. Już nigdy nie przegapisz żadnych dymów!",
	"homepage_url": "http://patostreamy.com/",
	"icons": {
		"16": "/icons/icon16.png",
		"19": "/icons/icon19.png",
		"48": "/icons/icon48.png",
		"128": "/icons/icon128.png"
	},
	"author": "Kamil @kamdz Dzwonkowski",
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_title": "Patostreamy.com Notifier",
		"default_icon": {
			"16": "/icons/icon16.png",
			"19": "/icons/icon19.png",
			"48": "/icons/icon48.png",
			"128": "/icons/icon128.png"
		}
	},
	"permissions": [
		"storage",
		"notifications",
		"https://patostreamy.herokuapp.com/*"
	]
}
